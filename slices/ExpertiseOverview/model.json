              "title": {
                "type": "Text",
                "config": {
                  "label": "Service Title"
                }
              }
            }
          }
        },
        "services_right": {
          "type": "Group",
          "config": {
            "label": "Services Right",
            "fields": {
              "icon": {
                "type": "Image",
                "config": {
                  "label": "Service Icon"
                }
              },
              "title": {
                "type": "Text",
                "config": {
                  "label": "Service Title"
                }
              }
            }
          }
        },
        "description": {
          "type": "StructuredText",
          "config": {
            "multi": "paragraph,strong,em,hyperlink",
